#include <stdio.h>

int main(){
  // Declarando as variáveis
  int numVeiculos, numAcidentes, indiceAcidentes;
  
  // Entrada de Dados
  
  printf("Digite a quantidade de veículos presentes no Rio Grande do Sul: ");
  scanf("%d", &numVeiculos);

  printf("Digite a quantidade de acidentes com vítimas no Rio Grande do Sul: ");
  scanf("%d", &numAcidentes);

  // Cálculo
  indiceAcidentes = (numAcidentes / numVeiculos) * 100;

  // Saída de Dados
  printf("\nO índice de acidentes com vítimas em relação a quantidade de veículos foi de: %d%%", indiceAcidentes);
}
